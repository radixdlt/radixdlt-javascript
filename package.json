  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radixdlt/radixdlt-javascript.git"
  },
  "scripts": {
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "build": "yarn build:ts",
    "build:ts": "tsc -b -f tsconfig.json",
    "bundle": "ts-node -P scripts/tsconfig.json scripts/bundle.ts umd,esm",
    "baseTest": "jest --rootDir=.",
    "test": "yarn baseTest -c jest.config.js --testTimeout=10000",
    "test:baseIntegration": "yarn baseTest --runInBand --testTimeout=30000 --verbose true",
    "test:integration": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn test:baseIntegration -c jest.config.integration.js",
    "test:ledger": "yarn test:baseIntegration --coverage=false -c jest.config.ledger.js",
    "test:ledger:hw": "yarn test:ledger ./src/hardware-ledger/test/ledgerNano.test.integration.ts",
    "test:ledger:account": "yarn test:ledger ./src/hardware-ledger/test/signingKeychain.test.integration.ts",
    "test:ledger:app": "yarn test:ledger ./src/hardware-ledger/test/radix.test.integration.ts",
    "format": "prettier --write '**/*.{ts,tsx,js}' --config .prettierrc",
    "sanity": "yarn build && yarn test"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-node-hid": "^6.20.0",
    "@open-rpc/client-js": "^1.7.1",
    "@radixdlt/open-rpc-spec": "^1.0.19",
    "@radixdlt/uint256": "^1.1.0",
    "@ramda/pipe": "^0.26.1",
    "@types/bn.js": "^5.1.0",
    "@types/elliptic": "^6.4.14",
    "@types/hdkey": "^2.0.0",
    "@types/node-forge": "^0.10.10",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.4.5",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "bip39": "^3.0.4",
    "bn.js": "^5.2.0",
    "elliptic": "^6.5.4",
    "ethereum-public-key-to-address": "^0.0.5",
    "hdkey": "^2.0.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "long": "^5.2.0",
    "neverthrow": "^4.3.1",
    "node-forge": "^0.10.0",
    "open-rpc-utils": "^1.1.1",
    "prelude-ts": "^1.0.3",
    "ramda": "^0.27.1",
    "rxjs": "^7.5.1",
    "scrypt-js": "^3.0.1",
    "sodium-native": "^3.3.0",
    "uuid": "^8.3.2"
  }
}
